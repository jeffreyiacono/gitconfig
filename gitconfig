[alias]
  amend = commit --amend
  co = checkout
  df = diff
  br = branch
  st = status
  s  = status
  ci = commit
  c  = commit
  cm = commit -m
  l  = log
  w  = whatchanged
  oneline = log --pretty=oneline
  la = log --pretty=\"format:%ad %h (%an): %s\" --date=short
  last = log -1 HEAD
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local   = yellow
  remote  = green
[color "diff"]
  meta = blue bold
  frag = magenta bold
  old  = red bold
  new  = green bold
[color "status"]
  added     = red
  changed   = green
  untracked = cyan
[diff]
  mnemonicprefix = true
[apply]
  whitespace = fix
[push]
  default = current
